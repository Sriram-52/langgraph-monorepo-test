{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "db:generate": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        ".next/cache/**"
      ]
    }
  }
}